package entity;

public class Account { //POJO class

	private int accNo;
	private String accHolderName;
	private double balance;
	
	public Account() {
		accNo=0;
		accHolderName=null;
		balance=0.0;
	}
	
	public Account(int accNo, String accHolderName, double balance) {
		super();
		this.accNo = accNo;
		this.accHolderName = accHolderName;
		this.balance = balance;
	}
	
	
	
}
